<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >

    <TextView
        android:id="@+id/lost_protect_tv_protectNumber"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:text="TextView"
        android:textSize="20sp" />

    <ImageView style="@style/lostProtect_prvision_line" />

    <CheckBox
        android:id="@+id/lost_protect_cb_isProtect"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/setupguide4_cb_content_off"
        android:textSize="20sp" />

    <Button
        android:id="@+id/lost_protect_btn_reSetup"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:text="@string/lost_protect_btn_reSetup"
        android:textSize="20sp" />

</LinearLayout>